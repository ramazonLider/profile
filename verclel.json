{
    "redirects": [
        {
            "source": "/old-path",
            "destination": "/new-path",
            "statusCode": 301
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}